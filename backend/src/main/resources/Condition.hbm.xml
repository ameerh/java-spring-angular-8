<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.reliant.value.test.model.Condition" table="CONDITION" schema="public" catalog="java_test">
        <id name="condition">
            <column name="CONDITION" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="value">
            <column name="VALUE" sql-type="numeric(5,2)" precision="5" scale="2"/>
        </property>
    </class>
</hibernate-mapping>